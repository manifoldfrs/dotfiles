{
  "compilerOptions": {
    "target": "es2016",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "outDir": "./lib",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["archive", "**/*.test.ts", "**/*.spec.ts", "src/test/**"],
  "include": ["src/", "files"]
}
