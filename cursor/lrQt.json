{
  "name": "@shaya/api",
  "version": "0.0.1",
  "description": "",
  "main": "lib/entry.js",
  "type": "module",
  "scripts": {
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --detectOpenHandles",
    "test:unit": "yarn node --experimental-vm-modules $(yarn bin jest) --selectProjects unit --detectOpenHandles --forceExit",
    "test:integration": "yarn node --experimental-vm-modules $(yarn bin jest) --selectProjects integration --runInBand --detectOpenHandles --forceExit",
    "test:watch": "yarn node --experimental-vm-modules $(yarn bin jest) --watch",
    "test:unit:watch": "yarn node --experimental-vm-modules $(yarn bin jest) --selectProjects unit --watch",
    "test:integration:watch": "yarn node --experimental-vm-modules $(yarn bin jest) --selectProjects integration --watch --runInBand",
    "dotenv:pull2": "npx dotenv-vault@latest pull",
    "dotenv:pull:dev": "esc env get shaya/service-dev --value dotenv --show-secrets > .env",
    "db:reset": "npx prisma db push --force-reset && npx prisma db seed",
    "milvus:reset": "node milvus/reset.mjs",
    "start": "nodemon --exec \"node .\"",
    "start:dev": "nodemon --watch \"./lib/**/*\" --exec \"node .\"",
    "build": "yarn run prisma generate; yarn run graphql:build; tsc;",
    "graphql:build": "graphql-codegen --config codegen.yml",
    "build:watch": "concurrently \"tsc --watch\" \"yarn run graphql:build --watch\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:lint": "yarn run format && yarn run lint",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
