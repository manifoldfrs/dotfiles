{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./lib",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["archive", "**/*.test.ts", "**/*.spec.ts", "src/test/**"],
  "include": ["src/**/*.ts"]
}
