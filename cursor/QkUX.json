{
  "name": "@shaya/tapestry-proto",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "yarn build:proto && yarn build:tsc",
    "build:watch": "yarn build:tsc --watch",
    "build:proto": "mkdir -p lib src/generated && protoc --proto_path=src --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts_proto\" --ts_opt=\"useExactTypes=true,outputClientImpl=grpc-web\" --ts_out=\"./src/generated\" src/tapestry.proto src/models.proto",
    "build:tsc": "tsc"
  },
  "devDependencies": {
    "protoc-gen-grpc-js": "^0.4.0",
    "protoc-gen-grpc-web": "^1.4.0",
    "protoc-gen-js": "^3.21.2",
    "protoc-gen-ts": "^0.8.7",
    "ts-proto": "^1.165.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.7.3",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@paperclip-ui/common": "^20.5.9",
    "@types/node": "^18.11.9",
    "browser-headers": "^0.4.1",
    "rxjs": "^7.5.7",
    "typescript": "^4.9.3"
  },
  "author": "",
  "license": "ISC"
}
